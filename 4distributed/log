   newtestdlltype ffunc_newtestdll =
            (newtestdlltype)GetProcAddress(testdll, "newtestdll") ;
   deletetestdlltype ffunc_deletetestdll =
            (deletetestdlltype)GetProcAddress(testdll, "deletetestdll") ;

