  {
  while (inputFile.good() && MisionCumplida == false)
    {
        i = 0;
        inputFile.getline(istr,256);
        pch = strtok (istr,":");
        if (string(pch) == "[Pagina]")
        {
            EncabezadoListo = true;
        }
        else
        {
            EncabezadoListo = false;
        }
        if (string(pch) == Page)
        {
            MisionCumplida = true;
        }
